#include <stdio.h>
#include <pthread.h>

void* fun(void* arg){
    printf("Thread %d is running\n", *(int*)arg);
    return NULL;
}

int main(){
    pthread_t t1, t2;
    int a=1, b=2;

    pthread_create(&t1, NULL, fun, &a);
    pthread_create(&t2, NULL, fun, &b);

    pthread_join(t1, NULL);
    pthread_join(t2, NULL);

    return 0;
}
