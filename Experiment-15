#include <stdio.h>

int main() {
    int n, start, len, i, j;
    int mem[100] = {0};

    scanf("%d", &n);

    for(i=1; i<=n; i++) {
        scanf("%d %d", &start, &len);
        printf("File %d allocated blocks: ", i);

        for(j=start; j < start+len; j++) {
            mem[j] = 1;
            printf("%d ", j);
        }
        printf("\n");
    }
}
