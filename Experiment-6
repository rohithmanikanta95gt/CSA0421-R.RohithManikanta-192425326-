#include <stdio.h>

int main() {
    int n, bt[20], rt[20], tat[20], wt[20];
    int i, q, time = 0, done;

    scanf("%d",&n);                    // number of processes
    for(i=0;i<n;i++) scanf("%d",&bt[i]); // burst times
    scanf("%d",&q);                    // time quantum

    for(i=0;i<n;i++) rt[i] = bt[i];    // copy burst times

    do {
        done = 1;
        for(i=0;i<n;i++) {
            if(rt[i] > 0) {
                done = 0;
                if(rt[i] > q) { time += q; rt[i] -= q; }
                else { time += rt[i]; tat[i] = time; rt[i] = 0; }
            }
        }
    } while(!done);

    for(i=0;i<n;i++)
        wt[i] = tat[i] - bt[i];

    printf("BT  WT  TAT\n");
    for(i=0;i<n;i++)
        printf("%d   %d   %d\n", bt[i], wt[i], tat[i]);

    return 0;
}
